      "email": "m@artemd.ru"
    }
  ],
  "require": {
    "php": ">=7.3",
    "ext-bcmath": "*",
    "elegant-bro/interfaces": "^1.1"
  },
  "require-dev": {
    "ext-simplexml": "*",
    "php-coveralls/php-coveralls": "^2.4",
    "phpstan/phpstan": "^1.8",
    "phpstan/phpstan-phpunit": "^1.1",
    "phpunit/phpunit": "^9.5",
    "roave/security-advisories": "dev-latest",
    "squizlabs/php_codesniffer": "^3.5"
  },
  "autoload": {
    "psr-4": {
      "ElegantBro\\Money\\": "src"
    },
    "files": [
      "src/functions.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "ElegantBro\\Money\\Tests\\": "tests"
    }
  },
  "config": {
    "preferred-install": {
      "*": "dist"
    },
    "sort-packages": true,
    "optimize-autoloader": true
  }
}
